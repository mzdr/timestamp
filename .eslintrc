{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 13,
    "sourceType": "module"
  },
  "rules": {
    "import/extensions": "off",
    "import/no-relative-packages": "off",
    "max-len": "off",
    "no-console": "off",
    "no-restricted-syntax": "off"
  },
  "settings": {
    "import/core-modules": ["electron"]
  }
}
